            <li><a href="<?=charset_recode ( csv_link , 'UTF-8', '_WIDE_')?>">CSV</a></li>
            <li><a href="<?=charset_recode ( ntriples, 'UTF-8', '_WIDE_') ?>">N-Triples</a></li> 
            <li><a href="<?=charset_recode ( rdf_link, 'UTF-8', '_WIDE_') ?>">N3 (Turtle)</a></li>
